<h1>Students#show</h1>

<table class="table table-bordered table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Email</th>
      <th scope="col">Cohorts</th>
      <th scope="col">Edit</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td><%= @student.fname %></td>
      <td><%= @student.lname %></td>
      <td><%= @student.email %></td>
      <td>*</td>
      <td>*</td>
    </tr>
  </tbody>
</table>
