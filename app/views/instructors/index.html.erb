<h1>Instructors#index</h1>

<table class="table table-bordered table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Email</th>
      <th scope="col">Edit</th>
    </tr>
  </thead>
  <tbody>
    <% x = 0 %>
    <% @instructors.each do |i| %>
    <tr>
      <th scope="row"><%= x += 1 %></th>
      <td><%= i.fname %></td>
      <td><%= i.lname %></td>
      <td><%= i.email %></td>
      <td>*</td>
    </tr>
    <% end %>
  </tbody>
</table>
