<h1>Cohorts#new</h1>

<%= link_to("Back to Cohorts", cohorts_path, :class => 'back-link') %>

  <%= bootstrap_form_for @cohort do |f| %>
    <%= f.text_field :name %>
    <%= f.text_field :startdate %>
    <%= f.text_field :enddate %>
    <%= f.select :instructor_id, Instructor.all.ids %>
    <%= f.select :course_id, Course.all.ids %>
    <%= f.submit %>
  <% end %>
